{
  "name": "code-tool/jaeger-mongodb-collector",
  "description": "Jaeger mongodb time collectior",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^7.2 || ^8.0",
    "ext-mongodb": "^1.20.0 || ^2.0.0",
    "code-tool/jaeger-client-php": "^3"
  },
  "autoload": {
    "psr-4": {
      "CodeTool\\Jaeger\\MongoDb\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CodeTool\\Jaeger\\Tests\\MongoDb\\": "test/"
    },
    "files": [
      "test/functions.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  }
}
